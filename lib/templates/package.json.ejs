{
  "name": "<%- pkgName %>",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
  },
  "devDependencies": {
  },
  "scripts": {
    "predeploy": "cfn-package --template-file template.yaml --output-template-file output-template.yaml --s3-bucket <%- bucketName %><%
  if (prefix !== undefined) {
    %> --s3-prefix <%- prefix %><%
  }
%>",
    "deploy": "aws cloudformation deploy --stack-name <%- stackName %> --template-file output-template.yaml --capabilities CAPABILITY_NAMED_IAM",
    "start": "api-gateway-local start -r us-west-2 --template-file template.yaml"
  }
}
